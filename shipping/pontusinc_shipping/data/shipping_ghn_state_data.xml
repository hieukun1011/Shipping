			<field name="sequence">11</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_return" model="shipping.state">
			<field name="name">The goods are waiting to return to seller/merchant after 3 times delivery failed</field>
			<field name="code">return</field>
			<field name="sequence">12</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_return_transporting" model="shipping.state">
			<field name="name">The goods are being rotated</field>
			<field name="code">return_transporting</field>
