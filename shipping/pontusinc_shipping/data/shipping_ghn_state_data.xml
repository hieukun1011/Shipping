<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
		<record id="shipping_state_ready_to_pick" model="shipping.state">
			<field name="name">Shipping order has just been created</field>
			<field name="code">ready_to_pick</field>
			<field name="sequence">1</field>
			<field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

        <record id="shipping_state_pick" model="shipping.state">
			<field name="name">Shipper is coming to pick up the goods</field>
			<field name="code">picking</field>
			<field name="sequence">1</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_cancel" model="shipping.state">
			<field name="name">Shipping order has been cancelled</field>
			<field name="code">cancel</field>
			<field name="sequence">2</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_money_collect_picking" model="shipping.state">
			<field name="name">Shipper are interacting with the seller</field>
			<field name="code">money_collect_picking</field>
			<field name="sequence">3</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_picked" model="shipping.state">
			<field name="name">Shipper is picked the goods</field>
			<field name="code">picked</field>
			<field name="sequence">4</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_storing" model="shipping.state">
			<field name="name">The goods has been shipped to GHN sorting hub</field>
			<field name="code">storing</field>
			<field name="sequence">5</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_transporting" model="shipping.state">
			<field name="name">The goods are being rotated</field>
			<field name="code">transporting</field>
			<field name="sequence">6</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_sorting" model="shipping.state">
			<field name="name">The goods are being classified (at the warehouse classification)</field>
			<field name="code">sorting</field>
			<field name="sequence">6</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_delivering" model="shipping.state">
			<field name="name">Shipper is delivering the goods to customer</field>
			<field name="code">delivering</field>
			<field name="sequence">7</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_money_collect_delivering" model="shipping.state">
			<field name="name">Shipper is interacting with the buyer</field>
			<field name="code">money_collect_delivering</field>
			<field name="sequence">8</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_delivered" model="shipping.state">
			<field name="name">The goods has been delivered to customer</field>
			<field name="code">delivered</field>
			<field name="sequence">9</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_delivery_fail" model="shipping.state">
			<field name="name">The goods hasn't been delivered to customer</field>
			<field name="code">delivery_fail</field>
			<field name="sequence">10</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
