		<record id="shipping_state_return_sorting" model="shipping.state">
			<field name="name">The goods are being classified (at the warehouse classification)</field>
			<field name="code">return_sorting</field>
			<field name="sequence">14</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_returning" model="shipping.state">
			<field name="name">The shipper is returning for seller</field>
			<field name="code">returning</field>
			<field name="sequence">15</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_return_fail" model="shipping.state">
			<field name="name">The returning is failed</field>
			<field name="code">return_fail</field>
			<field name="sequence">16</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_returned" model="shipping.state">
			<field name="name">The goods has been returned to seller/merchant</field>
			<field name="code">returned</field>
			<field name="sequence">16</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_exception" model="shipping.state">
			<field name="name">The goods exception handling</field>
			<field name="code">exception</field>
			<field name="sequence">17</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_damage" model="shipping.state">
			<field name="name">Damaged goods</field>
			<field name="code">damage</field>
			<field name="sequence">18</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

		<record id="shipping_state_lost" model="shipping.state">
			<field name="name">The goods are lost</field>
			<field name="code">lost</field>
			<field name="sequence">19</field>
            <field name="platform_id" ref="pontusinc_shipping.shipping_platform_ghn" />
		</record>

	</data>
</odoo>